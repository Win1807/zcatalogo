<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="ZCatalogoMesa.zcatalogo.controller.TipoAccionMain">
	<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="ACCIÓN PROMOCIONAL" subheader="" id="tile1" press="onAccion">
							<TileContent unit="CL" footer="Ver listado">
								<ImageContent src="sap-icon://process"/>
							</TileContent>
						</GenericTile>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="CATALOGO" subheader="" id="tile0" press="onCatalogo">
							<TileContent unit="CL" footer="Buscar Catalogo">
								<ImageContent src="sap-icon://picture"/>
							</TileContent>
						</GenericTile>
					</content>
					<footer>
					<Toolbar>
						<ToolbarSpacer/>
						<Button text="Versión 7.12.3" id="Guardar" press="onClick"/>
						
					</Toolbar>
				</footer>
				</Page>
			</pages>
		</App>
</mvc:View>