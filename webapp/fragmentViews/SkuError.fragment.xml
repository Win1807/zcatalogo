<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" >
	<Dialog title="Error SkU">
		<content>
		
					<Table items="{ModelLog>/DATA}" noDataText="Drop column list items here and columns in the area above" id="tableSkuError">
				<items>
					<ColumnListItem type="Active" >
						<cells>
							<Text text="{ModelLog>Sku}" />
							<Text text="{ModelLog>Mensaje}" />
						</cells>
					</ColumnListItem>
				</items>
				<columns>
					<Column id="column0">
						<header>
							<Label text="SKU" />
						</header>
					</Column>
					<Column id="column1">
						<header>
							<Label text="ERROR" />
						</header>
					</Column>
				</columns>
			</Table>
		</content>
		<buttons>
			<Button text="Volver" press="onCerrarFrag" type="Emphasized"/>
			
		</buttons>
				
				

		
	</Dialog>
</core:FragmentDefinition>