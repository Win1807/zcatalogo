<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog title="Listado precios por Centros" contentWidth="60%">
		<content>
			<Table noDataText="Drop column list items here and columns in the area above" id="table0" items="{ListCentro>/DATA}">
				<items>
					<ColumnListItem type="Active" id="item0">
						<cells>
							<Text text="{ListCentro>Zona}" />
							<Text text="{ListCentro>Centro}" />
							<Text text="{ListCentro>Preciovig}" />
							<Text text="{ListCentro>Moneda}" />
						</cells>
					</ColumnListItem>
				</items>
				<columns>
					<Column id="column0">
						<header>
							<Label text="ZONA" id="label0"/>
						</header>
					</Column>
					<Column id="column1">
						<header>
							<Label text="CENTRO" id="label1"/>
						</header>
					</Column>
					<Column id="column2">
						<header>
							<Label text="PRE VIGENTE" id="label2"/>
						</header>
					</Column>
					<Column id="column3">
						<header>
							<Label text="MONEDA" id="label3"/>
						</header>
					</Column>
				</columns>
			</Table>
		</content>
		<buttons>
			<Button text="Volver" press="onCerrarFrag" type="Emphasized"/>
			
		</buttons>
	</Dialog>
</core:FragmentDefinition>