<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" >
	<Dialog id="dialogError" title="Error" icon="sap-icon://message-error">
		<content>
				<Table items="{ModelLog>/DATA}" id="tableError">
				<items>
					<ColumnListItem type="Active" >
						<cells>
							<Text text="{ModelLog>Mensaje}" />
						</cells>
					</ColumnListItem>
				</items>
				<columns>
					<Column id="column1">
						<header>
							<Label text="ERROR" />
						</header>
					</Column>
				</columns>
			</Table>
		</content>
		<buttons>
			<Button text="Volver" press="onCerrarFrag" type="Emphasized"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>